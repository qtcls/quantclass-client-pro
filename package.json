	"version": "3.5.1-beta.3",
	"description": "Stock related service tool for Quantclass, including stock data download, stock data analysis, stock data visualization, etc.",
	"main": "./out/main/index.js",
	"exports": "./out/main/index.js",
	"type": "module",
	"repository": {
		"url": "http://gitlab.quantclass.cn/quantclass_private/download-react.git"
	},
	"engines": {
		"node": ">=20"
	},
	"homepage": "https://www.quantclass.cn/home",
	"author": {
		"name": "quantclass.cn",
		"email": "simons@quantclass.cn"
