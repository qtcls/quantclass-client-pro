	"scripts": {
		"typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
		"typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
		"typecheck": "npm run typecheck:node && npm run typecheck:web",
		"rebuild": "node bin/rebuild.js",
		"prebuild": "node bin/rebuild.js",
		"start": "electron-vite preview",
		"dev": "rm -rvf node_modules/.vite && cross-env VITE_XBX_ENV=development electron-vite dev && react-scan localhost:3000",
		"dev:win": "chcp 65001 && cross-env VITE_XBX_ENV=development electron-vite dev && react-scan localhost:3000",
		"dev:watch": "electron-vite dev --watch",
		"build": "electron-vite build",
		"postinstall": "electron-builder install-app-deps",
		"build:unpack": "cross-env VITE_XBX_ENV=production electron-vite build && electron-builder --dir",
		"build:win": "chcp 65001 && cross-env VITE_XBX_ENV=production electron-vite build && electron-builder --win",
		"build:mac": "cross-env VITE_XBX_ENV=production electron-vite build && electron-builder --mac",
		"build:linux": "cross-env VITE_XBX_ENV=production electron-vite build && electron-builder --linux",
		"publish:win": "chcp 65001 && cross-env VITE_XBX_ENV=production electron-vite build && electron-builder --win --publish always",
		"publish:mac": "cross-env VITE_XBX_ENV=production electron-vite build && electron-builder --mac --publish always",
		"publish:linux": "cross-env VITE_XBX_ENV=production electron-vite build && electron-builder --linux --publish always",
		"build:windev": "chcp 65001 && cross-env VITE_XBX_ENV=development electron-vite build && electron-builder --win"
	},
